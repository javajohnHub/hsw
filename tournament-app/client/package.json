{
  "name": "rnd-overlay-tournament-management",
  "version": "1.0.0",
  "description": "Tournament Management System for RND Overlay",
  "author": "RND Overlay",
  "homepage": "./",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel start:ui start:server",
    "start:ui": "ng serve --port 4300 --host 0.0.0.0",
    "start:ui:fast": "ng serve --configuration=development --port 4300 --host 0.0.0.0 --poll=2000 --live-reload=false --hmr=false",
    "start:server": "cross-env NODE_ENV=development node ../server/index.js",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
  "serve:prod": "ng build --configuration production && node ../server/index.js"
    
  },
  "dependencies": {
    "@angular/common": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.14",
    "@angular/cli": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/compiler-cli": "^19.2.14",
    "@angular/localize": "^19.2.14",
  "concurrently": "^8.0.0",
    "cross-env": "^7.0.3",
    
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.0",
    "wait-on": "^7.0.0"
  }
}
